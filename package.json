{
  "name": "mockprep",
  "version": "1.0.0",
  "description": "Exam preparation platform for CBSE and Cambridge students",
  "main": "dist/index.js",
  "scripts": {
    "build": "vite build",
    "build:backend": "tsc",
    "dev": "tsx watch src/index.ts",
    "dev:frontend": "vite",
    "build:frontend": "vite build",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "index:documents": "tsx scripts/indexDocuments.ts",
    "index:manual": "tsx scripts/manualIndexDocuments.ts"
  },
  "keywords": [
    "exam",
    "education",
    "cbse",
    "cambridge",
    "mock-test"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/jwt": "^7.2.4",
    "@prisma/client": "^5.7.1",
    "@supabase/supabase-js": "^2.93.2",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "axios": "^1.13.4",
    "bcrypt": "^5.1.1",
    "fastify": "^4.25.2",
    "pdf-parse": "^2.4.5",
    "pdfkit": "^0.14.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "vite": "^7.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.10.6",
    "@types/pdfkit": "^0.13.4",
    "@vitest/coverage-v8": "^1.1.0",
    "fast-check": "^3.15.0",
    "prisma": "^5.7.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
